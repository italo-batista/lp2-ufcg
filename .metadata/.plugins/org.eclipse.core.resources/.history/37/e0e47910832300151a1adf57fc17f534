

import java.util.List;

import exceptions.EntradaException;
import exceptions.TipoUsuarioException;

public class UsuarioFactory {

	public Usuario criaUsuario(String nome, String login, List<Jogo> jogosComprados, double dinheiro) throws EntradaException, TipoUsuarioException {
		Usuario usuario = new Usuario(nome, login, jogosComprados, dinheiro);		
		return usuario;
	}

}
