package sp2fy;

public class Musica {

	private String titulo;
	private int duracao;
	private String genero;

	public Musica(String titulo, int duracao, String genero) {
		this.titulo = titulo;
		this.duracao = duracao;
		this.genero = genero;
	}

	@Override
	public boolean equals(Object object) {
		if (object instanceof Musica) {
			Musica outraMusica = (Musica) object;
			if (this.titulo.equals(outraMusica.titulo)
				&& this.duracao == outraMusica.duracao) {
				return true;
			}
		}
		return false;
	}
	
	
}

