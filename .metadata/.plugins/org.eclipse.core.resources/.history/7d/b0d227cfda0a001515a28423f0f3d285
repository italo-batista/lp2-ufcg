package lab05;

import java.util.List;

public class UsuarioFactory {

	public Noob criaUsuarioNoob(String nome, String login, List jogosComprados, double dinheiro){
		Noob usuario = new Noob(String nome, String login, List jogosComprados, double dinheiro); 
		
		return usuario;
	}

	public Veterano criaUsuarioVeterano(String nome, String login, List jogosComprados, double dinheiro){
		Veterano usuario = null;
		
		return usuario;
	}
}
