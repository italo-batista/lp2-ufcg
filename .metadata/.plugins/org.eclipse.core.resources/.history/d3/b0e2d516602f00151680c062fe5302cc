public class Coordenador extends Gerente {
	private String nome;
	public static final double ADIC_BOLSA = 100.0;

	public Coordenador(String nome, double salarioBruto) {
		super(nome, salarioBruto);
		this.nome = nome;
	}

	public String getNome() {
		return this.nome;
	}

	public double calculaSalarioLiquido() {
		double salarioLiquido = super.salarioBruto;
		salarioLiquido = salarioLiquido + ADIC_BOLSA;
		return salarioLiquido;
	}
}